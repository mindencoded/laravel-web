version: '3'
rpc:
  listen: 'tcp://127.0.0.1:6001'
server:
  command: 'php app.php'
  relay: pipes
http:
  address: '0.0.0.0:8000'
  fcgi:
    address: tcp://0.0.0.0:9001
  middleware:
    - gzip
    - static
  static:
    dir: public
    forbid:
      - .php
      - .htaccess
  pool:
    debug: true
    num_workers: 100
    allocate_timeout: '1h'
    supervisor:
      max_worker_memory: 100
jobs:
  pool:
    num_workers: 2
    max_worker_memory: 100
  consume: { }
kv:
  local:
    driver: memory
    config:
      interval: 60
metrics:
  address: '127.0.0.1:2112'
logs:
  level: debug
  mode: development
